    "class HumanBase(Agent):\n",
    "    \"\"\"An agent class.\n",
    "    \n",
    "        unique_id: Agent id\n",
    "        age: (int) Age\n",
    "        model: The model that calls the agent\n",
    "        agent_type: name of the self class\n",
    "        infection_list: (list of strains) list of strains that agent is being infected \n",
    "        immune_list: (list of strains) list of strains that agent has immunity against\n",
    "        leftRecoveryTime: (list of int with length of infection_list) list of lest infectious duration \n",
    "            for each strain that agent is carrying\n",
    "        \n",
    "    \"\"\"\n",
    "    \n",
    "    def __init__(self,unique_id, model,agent_type, infection_list, immune_list,\n",
    "                 leftRecoveryTime,age, \n",
    "                ):       \n",
    "        super().__init__(unique_id,model)\n",
    "        \n",
    "        self.agent_type = agent_type\n",
    "        #strains list that agent is infected by\n",
    "        self.infection_list = infection_list\n",
    "        #strains list that agent is immune to\n",
    "        self.immune_list = immune_list\n",
    "        #recovery time list for each infections\n",
    "        self.leftRecoveryTime = leftRecoveryTime\n",
    "        self.age = age #agent's current age\n",
    "        #the strain list that agent going to be infected by in the next step\n",
    "        self.willInfected_list = [0]*len(self.model.strain_list) \n",
    "        #the strain list that agent going to be immune to in the next step\n",
    "        self.willRecovered_list = [0]*len(self.model.strain_list) \n",
    "        #the strain list that agent going to be susceptible to in the next step\n",
    "        self.willSusceptible_list = [0]*len(self.model.strain_list) \n",
    "        #whether the agent is going to die in the next step\n",
    "        self.willDie = 0\n",
    "        #whether the agent is going to give birth in the next step\n",
    "        self.willGiveBirth = 0\n",
    "        #left immunity time list for the past infections\n",
    "        self.leftImmunityTime = [] #no immunity at the beginning\n",
    "        self.cumnoinfections = 0 #cumulative number of infections \n",
    "        #whether the agent going to migrate in the next time step\n",
    "        self.willMigrate = False\n",
    "        self.checkdeathweek = self.model.random.randint(0,time_step_to_year -1)\n",
    "        #count # of agents that this agent infected by each strain\n",
    "        self.noSpread_list = []\n",
    "        self.willSpread_list = []\n",
    "        #count total number of spread by any strain during an infectious period between being S to being R/S\n",
    "        self.totalnoSpread = 0\n",
    "        self.totalwillSpread = 0\n",
    "        \n",
    "    def amISusceptibleAT(self,strain):\n",
    "        \"\"\"returns True if the agent is antigenically susceptible to exposed AT in every loci. \n",
    "        Function compares the AT loci of given strain and \"agent's current infections\n",
    "         and immunity strains. \"\"\"\n",
    "        \n",
    "        a_list = self.immune_list + self.infection_list     \n",
    "        if not a_list:\n",
    "            #agent has no immunity againts any ATs\n",
    "            return True\n",
    "        else:\n",
    "            f = 0     #f: number of loci having the same values in AT loci\n",
    "            for h in range(self.model.slicing_AT_start,self.model.slicing_AT_end):\n",
    "                s = strain[h]\n",
    "                i = [a_list[k][h] for k,l in enumerate(a_list)]\n",
    "                if s in i:\n",
    "                    f += 1\n",
    "            #return true if there arent any shared loci\n",
    "            #between exposed and infected strains\n",
    "            return f == 0\n",
    "    \n",
    "    def amISusceptibleMT(self,strain):\n",
    "        \"\"\"\n",
    "        returns True if the agent is metabolically susceptible to exposed strain. \n",
    "        Function compares the MT mechanisms of given strain and agent's current infections\n",
    "        strains.\n",
    "        \"\"\" \n",
    "        a_list=self.infection_list \n",
    "        if not a_list:\n",
    "            #agent has no infections\n",
    "            return True\n",
    "        else:\n",
    "            f=0 #f: number of loci list having the same values in MT loci\n",
    "            for h in range(self.model.slicing_MT_start,self.model.slicing_MT_end):\n",
    "                s = strain[h]\n",
    "                i = [a_list[k][h] for k,l in enumerate(a_list)]\n",
    "                if s in i:\n",
    "                    f += 1\n",
    "            #return true if there arent any shared loci\n",
    "            #between exposed and infected strains\n",
    "            return f == 0\n",
    "        \n",
    "    def amISusceptible(self, strain):\n",
    "        \"\"\"checks if the agent is both antigenically and metabolically susceptible to a given strain.\"\"\"\n",
    "        #control for number of co-infections. Number of co infections\n",
    "        #cannot be higher than a given max value.\n",
    "        value = 1\n",
    "        if len(self.infection_list) < self.model.max_no_infections:\n",
    "\n",
    "            if self.model.no_loci_AT > 0:\n",
    "                #only have MT types\n",
    "                value = value * self.amISusceptibleAT(strain)\n",
    "                if not value:\n",
    "                    return value\n",
    "            \n",
    "            if self.model.no_loci_MT > 0:\n",
    "                #only have MT types\n",
    "                value = value * self.amISusceptibleMT(strain)\n",
    "            \n",
    "            return value\n",
    "        else:\n",
    "            return 0\n",
    "        \n",
    "    def amIImmune(self, strain):\n",
    "        \"\"\"\n",
    "        amIImmune function checks whether the agent is immune to a given strain or not. \n",
    "        The agent compares its current immune list with the given strain and return 0 or 1.\n",
    "        \"\"\"\n",
    "        if self.model.no_loci_AT == 0:\n",
    "            #strains only have MT types therefore there is no antigenic memory of strains; and no immunity\n",
    "            return 0\n",
    "        else:#there is at lest one AT loci of strains\n",
    "            #return True if the agent is antigenically not susceptible \n",
    "            if not self.immune_list: #no immunity agains any strains\n",
    "                return 0\n",
    "            else: \n",
    "                for imm_strain in self.immune_list:\n",
    "                    if imm_strain[self.model.slicing_AT_start:self.model.slicing_AT_end] ==\\\n",
    "                     strain[self.model.slicing_AT_start:self.model.slicing_AT_end]:\n",
    "                        return 1\n",
    "                #no common AT is found\n",
    "                return 0\n",
    "     \n",
    "    def give_birth(self):\n",
    "        n_id=self.model.next_id\n",
    "        #no vertical infection from parent to child\n",
    "        a = self.agent_type(n_id, self.model,self.agent_type, infection_list = [], immune_list = [],\n",
    "                  age = 0,\n",
    "                  leftRecoveryTime = [])\n",
    "        a.checkdeathweek = self.model.time % time_step_to_year \n",
    "\n",
    "        self.model.schedule.add(a) \n",
    "        #create new node
    "        list_of_nodes = [node_id for node_id in self.model.G.nodes if 
    "                          self.model.random.random() < self.model.prob]
    "        self.model.G.add_node(n_id)
    "        self.model.G.add_edges_from((n_id,i) for i in list_of_nodes)
    "        #assign new agent 'a' to new node
    "        self.model.G.nodes[n_id]['agent'] = None
    "        self.model.G.nodes[n_id]['agent'] = a
    "        self.model.next_id=self.model.next_id+1
    "
    "    \n",
    "    def check_die(self):\n",
    "        \"\"\"return whether the agent going to die and give birth in the next step\"\"\"\n",
    "        self.age += 1\n",
    "        if self.age < len(self.model.deathrates):\n",
    "            cur_deathrate = self.model.deathrates[self.age]\n",
    "        else:#person is older than 100 year old, use last death rate\n",
    "            cur_deathrate = self.model.deathrates[-1]\n",
    "            \n",
    "        if self.model.random.random() <= cur_deathrate:\n",
    "            self.willDie = True\n",
    "            self.willGiveBirth = True\n",
    "            \n",
    "    def check_migrate(self):\n",
    "        \"\"\"return whether the agent is going to migrate or not in the next step\"\"\"\n",
    "        if self.model.random.random() < (self.model.migration_rate):\n",
    "            self.willMigrate = True\n",
    "            self.model.noEmmigratedAgents += 1\n",
    "        \n",
    "            \n",
    "    def migrate(self):\n",
    "        #migrate self by removing the agent self\n",
    "        self.model.schedule.remove(self) \n",
    "        #network\n",
    "        self.model.G.remove_node(self.unique_id)\n",
    "        \n",
    "    \n",
    "    def die(self):\n",
    "        \"\"\"removes agent from agent list and removes agent's node from the network.\"\"\"\n",
    "        self.model.schedule.remove(self) \n",
    "        self.model.G.remove_node(self.unique_id)\n",
    "            \n",
    "                \n",
    "    def becomeSusceptible(self,strain):   \n",
    "        \"\"\"moves agent from recovered state to susceptible state (losing imminity)\"\"\"\n",
    "        if strain in self.immune_list:\n",
    "            self.leftImmunityTime.remove(self.leftImmunityTime[\n",
    "            self.immune_list.index(strain)])\n",
    "            self.immune_list.remove(strain)\n",
    "        #if strain in self.infection_list:\n",
    "        #    self.infection_list.remove(strain)\n",
    "        \n",
    "    \n",
    "    def becomeInfected(self,strain):\n",
    "        \"\"\"moves agent from infected to recovered state\"\"\"\n",
    "        self.cumnoinfections += 1\n",
    "        self.infection_list.append(strain)\n",
    "        #recovery time in case of multiple infections\n",
    "        self.leftRecoveryTime.append(round(float(self.model.np_rng.gamma(shape=infection_shape, \n",
    "                                    scale=self.model.avg_recovery_time/infection_shape, size=1))))\n",
    "        #add a count of # of agents infect by self by that strain\n",
    "        self.noSpread_list.append(0)\n",
    "        self.willSpread_list.append(0)\n",
    "        \n",
    "\n",
    "    def becomeRecovered(self,strain):\n",
    "        \"\"\"moves agent from infected state to recovered state\"\"\"\n",
    "        #when recovered add number of agents that I infected with that strain in model reff list\n",
    "        self.model.reff_dict[\"%s\"%strain].append(self.noSpread_list[\n",
    "            self.infection_list.index(strain)])\n",
    "        #self.strain_list[k][0:self.no_loci_AT]\n",
    "        self.model.reff_at_dict[\"%s\"%strain[self.model.slicing_AT_start:self.model.slicing_AT_end]]\\\n",
    "        .append(self.noSpread_list[self.infection_list.index(strain)])\n",
    "        \n",
    "        #remove given strain and its related rec time/noof spread values\n",
    "        self.leftRecoveryTime.remove(self.leftRecoveryTime[\n",
    "            self.infection_list.index(strain)])\n",
    "        self.noSpread_list.remove(self.noSpread_list[\n",
    "            self.infection_list.index(strain)])\n",
    "        self.willSpread_list.remove(self.willSpread_list[\n",
    "            self.infection_list.index(strain)])\n",
    "        self.infection_list.remove(strain)\n",
    "        \n",
    "        #if there isnt any strainleft: agent will be immune or recovered\n",
    "        if len(self.infection_list) == 0:\n",
    "            self.model.reff_pathogen.append(self.totalnoSpread)\n",
    "            self.totalnoSpread=0\n",
    "            self.totalwillSpread=0\n",
    "                    \n",
    "        if self.model.immunity_duration != 0: #if there is immunity\n",
    "            #check if there are other strains with the same AT in current infections\n",
    "            if len(self.infection_list) > 0:\n",
    "                for other_strain in self.infection_list:\n",
    "                    if strain[self.model.slicing_AT_start:self.model.slicing_AT_end] == \\\n",
    "                    other_strain[self.model.slicing_AT_start:self.model.slicing_AT_end]:\n",
    "                        \n",
    "                        #other strain has the same AT so remove that infection too\n",
    "                        #when recovered add number of agents that I infected with that strain in model reff list\n",
    "                        #when recovered add number of agents that I infected with that strain in model reff list\n",
    "                        self.model.reff_dict[\"%s\"%other_strain].append(self.noSpread_list[\n",
    "                            self.infection_list.index(other_strain)])\n",
    "                        self.model.reff_at_dict[\"%s\"%other_strain\\\n",
    "                                                [self.model.slicing_AT_start:self.model.slicing_AT_end]]\\\n",
    "                        .append(self.noSpread_list[self.infection_list.index(other_strain)])\n",
    "\n",
    "                        #remove given strain and its related rec time/noof spread values\n",
    "                        self.leftRecoveryTime.remove(self.leftRecoveryTime[\n",
    "                            self.infection_list.index(other_strain)])\n",
    "                        self.noSpread_list.remove(self.noSpread_list[\n",
    "                            self.infection_list.index(other_strain)])\n",
    "                        self.willSpread_list.remove(self.willSpread_list[\n",
    "                            self.infection_list.index(other_strain)])\n",
    "                        self.infection_list.remove(other_strain)\n",
    "\n",
    "                        #if there isnt any strainleft: agent will be immune or recovered\n",
    "                        if len(self.infection_list) == 0:\n",
    "                            self.model.reff_pathogen.append(self.totalnoSpread)\n",
    "                            self.totalnoSpread = 0\n",
    "                            self.totalwillSpread = 0\n",
    "                        \n",
    "            #add given strain to immune list\n",
    "            #don't have to add other strains since AT of strains will be the same\n",
    "            self.immune_list.append(strain)\n",
    "            #add left immunity time of that strain\n",
    "            self.leftImmunityTime.append(round(float(self.model.np_rng.gamma(shape= immunity_shape, \n",
    "                                                scale= self.model.immunity_duration/immunity_shape, size=1))))\n",
    "            \n",
    "             \n",
    "    def try_to_infect_neighbors(self, strain):\n",
    "        \"\"\"Try to infect neoighbors with a strain that agent carries\"\"\"\n",
    "        sus_neighbors_nodes = [n for n in self.model.G.neighbors(self.unique_id) \n",
    "                               if (self.model.G.nodes[n]['agent'].amISusceptible(\n",
    "                                   strain) == 1)] \n",
    "        \n",
    "        for node in sus_neighbors_nodes:\n",
    "            a=self.model.G.nodes[node]['agent']\n",
    "            probTrans= self.model.prob_of_transmission_list[\n",
    "                self.model.strain_list.index(strain)]\n",
    "            \n",
    "            #for fixed network graph\n",
    "            if self.model.random.random() < probTrans:\n",
    "                #s= [i for i,s in enumerate(self.model.strain_list) if s==strain]\n",
    "                s=self.model.strain_list.index(strain)\n",
    "                a.willInfected_list[s]=1\n",
    "                \n",
    "    \n",
    "    def try_being_infected(self): \n",
    "        \"\"\"if the agent is exposed to multiple strains, randomly select one of them to be infected.\"\"\"\n",
    "        #select only one strain to be infected from\n",
    "        if sum(self.willInfected_list) > 0:\n",
    "            slist = [i for i,s in enumerate(self.willInfected_list) if s == 1]\n",
    "            s = self.model.random.choice(slist) \n",
    "            #increase # of infected agents that is infected by the contact agent by 1\n",
    "            possible_inf_neighbors_nodes = [n for n in self.model.G.neighbors(self.unique_id) \n",
    "                               if (self.model.strain_list[s] in \n",
    "                                   self.model.G.nodes[n]['agent'].infection_list)]  \n",
    "            #infected agent is\n",
    "            inf_node = self.model.random.choice(possible_inf_neighbors_nodes) \n",
    "            #select the neighbor that infected self\n",
    "            a = self.model.G.nodes[inf_node]['agent']\n",
    "            #increase count of that neigbor by 1\n",
    "            a.willSpread_list[a.infection_list.index(self.model.strain_list[s])] += 1\n",
    "            #if I was susceptible to pathogen:\n",
    "            if len(self.infection_list) == 0:\n",
    "                a.totalwillSpread += 1\n",
    "             \n",
    "            for k in slist:\n",
    "                if k != s:\n",
    "                    self.willInfected_list[k] = 0    \n",
    "            \n",
    "       \n",
    "            \n",
    "    \n",
    "    def try_check_situation(self):\n",
    "        \"\"\"Update leftRecoveryTime list\"\"\"\n",
    "        for strain in self.infection_list:\n",
    "            if self.leftRecoveryTime[self.infection_list.index(strain)] is 0:\n",
    "                s = self.model.strain_list.index(strain)\n",
    "                self.willRecovered_list[s] = 1\n",
    "\n",
    "            else:\n",
    "                self.leftRecoveryTime[self.infection_list.index(strain)] -= 1\n",
    "\n",
    "        for strain in self.immune_list:\n",
    "            if self.leftImmunityTime[self.immune_list.index(strain)] == 0:\n",
    "                s = self.model.strain_list.index(strain)\n",
    "                self.willSusceptible_list[s] = 1        \n",
    "\n",
    "            else:\n",
    "                self.leftImmunityTime[self.immune_list.index(strain)] -= 1\n",
    "                \n",
    "    def agent_info_print(self):\n",
    "        \"\"\"used for debugging when self.model.agentDataTime != 0\"\"\"\n",
    "        if (self.model.agentDataTime != 0):\n",
    "            t=self.model.agentDataTime\n",
    "            if (self.model.time == t):\n",
    "                d100 = {'agent.ID': str(self.unique_id),\n",
    "                        'age'+str(t): str(self.age),\n",
    "                        'age'+str(t+timediff): \"\",\n",
    "                        'Infection List '+str(t): str(self.infection_list),\n",
    "                        'Infection List '+str(t+timediff): \"\",\n",
    "                        'leftRecoveryTime'+str(t):str(self.leftRecoveryTime),\n",
    "                        'leftRecoveryTime'+str(t+timediff):\"\",\n",
    "                        'Immune List '+str(t): str(self.immune_list),\n",
    "                        'Immune List '+str(t+timediff): \"\",\n",
    "                        'leftImmunityTime'+str(t):str(self.leftImmunityTime),\n",
    "                        'leftImmunityTime'+str(t+timediff):\"\",\n",
    "                       #'willInfected'+str(t): str(slist),\n",
    "                       'SpreadList'+str(t):str(self.noSpread_list),\n",
    "                       'SpreadList'+str(t+timediff):\"\",\n",
    "                       'PathogenSpread'+str(t):str(self.totalnoSpread),\n",
    "                       'PathogenSpread'+str(t+timediff):\"\"}   \n",
    "                \n",
    "\n",
    "                if (self.model.f100):\n",
    "                    self.model.df100=pd.DataFrame(data=d100, index=[0])\n",
    "                    self.model.f100= False\n",
    "                else:\n",
    "                    df102 = pd.DataFrame(data=d100, index=[0])\n",
    "                    self.model.df100=self.model.df100.append(df102,\n",
    "                                                             ignore_index = True) \n",
    "\n",
    "            elif (self.model.time == (t+timediff)):\n",
    "                #self.model.df100['Infection List 101'] = \"\"\n",
    "                if (str(self.unique_id) in self.model.df100['agent.ID'].tolist()):\n",
    "                    i100=self.model.df100[self.model.df100[\n",
    "                        'agent.ID']==str(self.unique_id)].index.tolist()\n",
    "                    self.model.df100.at[i100[0],'Infection List '+\n",
    "                                        str(t+timediff)]=str(self.infection_list)\n",
    "                    self.model.df100.at[i100[0],'leftRecoveryTime'+\n",
    "                                        str(t+timediff)]=str(self.leftRecoveryTime)\n",
    "                    self.model.df100.at[i100[0],'age'+str(t+timediff)]=str(self.age)\n",
    "                    self.model.df100.at[i100[0],'Immune List '+\n",
    "                                        str(t+timediff)]=str(self.immune_list)\n",
    "                    self.model.df100.at[i100[0],'leftImmunityTime'+ \n",
    "                                        str(t+timediff)]=str(self.leftImmunityTime)\n",
    "                    self.model.df100.at[i100[0],'SpreadList'+ \n",
    "                                        str(t+timediff)]=str(self.noSpread_list)\n",
    "                    self.model.df100.at[i100[0],'PathogenSpread'+ \n",
    "                                        str(t+timediff)]=str(self.totalnoSpread)\n",
    "                else:\n",
    "                    d100 = {'agent.ID': str(self.unique_id),\n",
    "                            'age'+str(t): \"\",\n",
    "                            'age'+str(t+timediff): str(self.age),\n",
    "                        'Infection List '+str(t): \"\",\n",
    "                        'Infection List '+str(t+timediff): str(self.infection_list),\n",
    "                        'leftRecoveryTime'+str(t):\"\",\n",
    "                        'leftRecoveryTime'+str(t+timediff):str(self.leftRecoveryTime),\n",
    "                        'Immune List '+str(t): \"\",\n",
    "                        'Immune List '+str(t+timediff): str(self.immune_list),\n",
    "                         'leftImmunityTime'+str(t):\"\",\n",
    "                         'leftImmunityTime'+str(t+timediff):str(self.leftImmunityTime),\n",
    "                        #'willInfected'+str(t): \"\",\n",
    "                           'SpreadList'+str(t):\"\",\n",
    "                       'SpreadList'+str(t+timediff):str(self.noSpread_list),\n",
    "                           'PathogenSpread'+str(t+timediff):str(self.totalnoSpread)} \n",
    "                    df102 = pd.DataFrame(data=d100, index=[0])\n",
    "                    self.model.df100=self.model.df100.append(df102, \n",
    "                                                             ignore_index = True) \n",
    "        \n",
    "        \n",
    "    def step1(self):\n",
    "        \"\"\"First agent step\"\"\"\n",
    "        self.willSpread_list = self.noSpread_list.copy()\n",
    "        self.totalwillSpread = self.totalnoSpread\n",
    "        if len(self.infection_list) > 0:\n",
    "            s = self.model.random.choice(self.infection_list)\n",
    "            self.try_to_infect_neighbors(s)\n",
    "                \n",
    "    def step2(self):\n",
    "        \"\"\"Second agent step\"\"\"\n",
    "        self.try_being_infected()\n",
    "        self.try_check_situation()\n",
    "        self.check_migrate()\n",
    "        if not self.willMigrate: #if the agent wont migrate, then chech whether the agent dies or not\n",
    "            if self.model.birthdeath: #if there is a defined birth-death process\n",
    "                if  self.model.time % time_step_to_year  == self.checkdeathweek:\n",
    "                    self.check_die()\n",
    "    \n",
    "    def advance(self):\n",
    "        \"\"\"Third agent step\"\"\"\n",
    "        slist = [i for i,s in enumerate(self.willInfected_list) if s == 1]\n",
    "        rlist = [i for i,r in enumerate(self.willRecovered_list) if r == 1]\n",
    "        ilist = [i for i,m in enumerate(self.willSusceptible_list) if m == 1]\n",
    "    \n",
    "        if len(slist) > 0:\n",
    "            s = self.model.random.choice(slist) \n",
    "            self.becomeInfected(self.model.strain_list[s])\n",
    "                \n",
    "        for s in slist:\n",
    "            self.willInfected_list[s] = 0       \n",
    "            \n",
    "        for r in rlist:\n",
    "            self.becomeRecovered(self.model.strain_list[r])\n",
    "            self.willRecovered_list[r] = 0\n",
    "            \n",
    "        for m in ilist:\n",
    "            self.becomeSusceptible(self.model.strain_list[m])\n",
    "            self.willSusceptible_list[m] = 0\n",
    "        \n",
    "        self.noSpread_list = self.willSpread_list.copy()\n",
    "        self.totalnoSpread = self.totalwillSpread\n",
    "        self.agent_info_print()    \n",
    "        \n",
    "        if self.willMigrate: \n",
    "            if self.model.migration_rate > 0: #extra control check\n",
    "            #the agent will migrate\n",
    "                self.migrate()\n",
    "        else: #the agent does not migrate    \n",
    "            if self.model.birthdeath: #if there is a defined birth-death process\n",
    "                if self.willGiveBirth:\n",
    "                    self.give_birth()\n",
    "                    self.willGiveBirth = False\n",
    "                    \n",
    "                if self.willDie:\n",
    "                    #self.willDie=False\n",
    "                    self.die()"
